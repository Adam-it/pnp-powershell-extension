{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6",
			"dom"
		],
		"sourceMap": true,
		"strict": false,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
	},
	"exclude": [
		"node_modules",
		".vscode-test",
		"webview-ui",
		"models"
	]
}